    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Mavenproject1",
            "request": "launch",
            "mainClass": "com.mycompany.mavenproject1.Mavenproject1"
        },
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "launch",
            "program": "C:/Users/ADMIN/Desktop/Txt Docs & Scripts/C++/personal-projects/ds/FULL/filesysman.exe",
            "MIMode": "gdb",
            "miDebuggerPath": "/path/to/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}